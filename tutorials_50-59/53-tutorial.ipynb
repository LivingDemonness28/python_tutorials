    "def food():\n",
    "    foods_dict = {}\n",
    "    \n",
    "    i = 1\n",
    "    j = 0\n",
    "    \n",
    "    while (i <= 4):\n",
    "        foods = input(f\"Please enter your food ({i}): \").strip().title()\n",
    "        foods_dict[j + 1] = foods\n",
    "        j += 1\n",
    "        i += 1\n",
    "        \n",
    "    print(foods_dict)"
